class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        if( !head || !head->next ){
            return head;
        }
        ListNode* s = head;
        ListNode* f = head;
        while( f && f->next ){
            s = s->next;
            f = f->next->next;
        }
        return s;
    }
};
